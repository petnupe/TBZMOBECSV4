<?xml version='1.0' encoding='utf-8'?>
<widget codeVersion="3.0.0" id="com.tecbiz.autorizador" version="3.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>TecBiz Lojista</name>
    <description>
        Aplicativo destinado a rede conveniada TecBiz para realização de autorizações, estornos e consultas.
    </description>
    <author email="tecnologia@tecbiz.com.br" href="http://tecbiz.com.br">
        Equipe TecBiz
    </author>
    <content src="index.html" />
    <access origin="http://www2.tecbiz.com.br" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
